    "@mui/material": ">=5",
    "@mui/x-data-grid": ">=5",
    "@seneca/search-mini": "0.1.6",
    "material-react-table": "^1.14.0",
    "patrun": "7.2.5",
    "react": ">=18",
    "react-dom": ">=18",
    "react-hook-form": "^7.45.4",
    "react-redux": ">=8",
    "react-router-dom": ">=6",
    "styled-components": ">=5"
  },
  "devDependencies": {
    "@babel/core": "7.22.10",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@fontsource/roboto": "4.5.8",
    "@mdi/js": "7.1.96",
    "@mdi/react": "1.6.1",
    "@mui/icons-material": "5.11.9",
    "@mui/material": "5.11.10",
    "@mui/x-data-grid": "5.17.25",
