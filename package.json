    "react": ">=18",
    "react-dom": ">=18",
    "react-redux": ">=8",
    "react-router-dom": ">=6",
    "styled-components": ">=5"
  },
  "devDependencies": {
    "react-router-dom": "6.9.0",
    "@babel/core": "7.21.0",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@fontsource/roboto": "4.5.8",
    "@mdi/js": "7.1.96",
    "@mdi/react": "1.6.1",
